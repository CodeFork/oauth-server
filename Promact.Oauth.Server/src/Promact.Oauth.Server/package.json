    "@angular2-material/progress-bar": "2.0.0-alpha.7",
    "@angular2-material/progress-circle": "2.0.0-alpha.7",
    "angular2-in-memory-web-api": "0.0.15",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12",
    "md2": "1.0.14",
